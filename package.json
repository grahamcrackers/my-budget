{
    "name": "my-budget",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "local/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "dotenv -- turbo run dev --filter=!stories",
        "dev:storybook": "dotenv -- turbo run dev --filter=stories...",
        "test": "turbo run test",
        "test:watch": "turbo run test:watch",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "docker:setup": "docker compose up",
        "docker:destroy": "docker compose down -v",
        "docker:bash": "docker exec -it",
        "clean": "./scripts/cleanup.sh"
    },
    "devDependencies": {
        "dotenv-cli": "^7.0.0",
        "eslint-config-my-budget": "*",
        "prettier": "^2.8.4",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {},
    "packageManager": "yarn@1.22.17"
}
