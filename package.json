{
    "name": "my-budget",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "test": "turbo run test",
        "test:watch": "turbo run test:watch",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""

    },
    "devDependencies": {
        "eslint-config-custom": "*",
        "prettier": "^2.8.4",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {},
    "packageManager": "yarn@1.22.17"
}
