generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Budget {
    id        String   @id @default(uuid())
    name      String
    created   DateTime @default(now())
    createdBy String   @db.Uuid
    updated   DateTime @updatedAt
    updatedBy String   @db.Uuid

    @@map("budgets")
}
